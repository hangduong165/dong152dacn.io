{
  "name": "Smart Search",
  "description": "Extension For You",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2,

  "content_scripts": 
  [
    {
    "matches": ["https://*.zendvn.com/*"],
    "css":["myStyle.css"],
    "js":["popup.js"]
    }
  ]
}
